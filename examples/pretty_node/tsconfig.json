{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["ES2016"],
    "types": ["@globe/runtime_types", "node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
